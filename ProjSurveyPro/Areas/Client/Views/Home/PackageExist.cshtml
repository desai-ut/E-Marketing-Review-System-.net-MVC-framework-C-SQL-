<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <meta name="keywords" content="HTML5 Admin Template" />
    <meta name="description" content="Porto Admin - Responsive HTML5 Template">
    <meta name="author" content="okler.net">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/Contents/assets/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Contents/assets/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/Contents/assets/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/Contents/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/Contents/assets/stylesheets/theme.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/Contents/assets/stylesheets/theme-custom.css">

    <!-- Head Libs -->
    <script src="~/Contents/assets/vendor/modernizr/modernizr.js"></script>
</head>
<body>
    <!-- start: page -->
    <section class="body-sign">
        <div class="center-sign">
            <a href="@Url.Action("Index", "Home", new { area = "Client" })" class="logo pull-left">
                <img src="~/Contents/assets/images/Logo.png"
                    height="35" alt="Survey Logo"
                    style="width: 160px; height: 75px; margin-top: -6px; animation-iteration-count: 3;"
                    class="appear-animation bounceIn appear-animation-visible"
                    data-appear-animation="bounceIn"
                    data-appear-animation-duration="10s" />
            </a>
            <div style="width: 650px; height: 400px">
                <div class="panel panel-sign">
                    <div class="panel-title-sign mt-xl text-right">
                        <h2 class="title text-uppercase text-bold m-none"><i class="fa  fa-exclamation-triangle"></i>&nbsp;   &nbsp; Already Have Package</h2>
                    </div>
                    <div class="panel-body">
                        <div class="alert alert-success">
                            @if (ViewBag.EndingDate != null)
                            {
                                <p class="m-none text-semibold h6" style="font-size: medium; line-height: 25px;">
                                    Welcome to SurveyPro..You Successfully Purchase @ViewBag.PackageTitle Package...
                                Which Have @ViewBag.ProductCnt Product Left to add in that Package...And Last Date of that Package is @ViewBag.EndingDate...
                                </p>
                            }
                            else
                            {
                                <p class="m-none text-semibold h6" style="font-size: medium; line-height: 25px;">
                                    Welcome to SurveyPro..You Already Successfully Purchase @ViewBag.PackageTitle...
                                Which Have @ViewBag.ProductCnt Product Left in that Package...Wait for activation of your Package
                                </p>
                            }
                        </div>
                        <div>

                            <a href="@Url.Action("Index", "Home", new { area = "Client" })" class="mb-xs mt-xs btn btn-info">Back To Home</a>
                        </div>
                    </div>
                </div>
            </div>
            <p class="text-center text-muted mt-md mb-md">&copy; Copyright 2017. All Rights Reserved.</p>
        </div>
    </section>
    <!-- end: page -->

    <!-- Vendor -->
    <script src="~/Contents/assets/vendor/jquery/jquery.js"></script>
    <script src="~/Contents/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/Contents/assets/vendor/jquery-cookie/jquery.cookie.js"></script>
    <script src="~/Contents/assets/vendor/style-switcher/style.switcher.js"></script>
    <script src="~/Contents/assets/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/Contents/assets/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="~/Contents/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Contents/assets/vendor/magnific-popup/magnific-popup.js"></script>
    <script src="~/Contents/assets/vendor/jquery-placeholder/jquery.placeholder.js"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="~/Contents/assets/javascripts/theme.js"></script>

    <!-- Theme Custom -->
    <script src="~/Contents/assets/javascripts/theme.custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/Contents/assets/javascripts/theme.init.js"></script>
