<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

<script type="text/javascript">
    google.charts.load('current', { packages: ['corechart', 'bar'] });
    google.charts.setOnLoadCallback(drawAnnotations);

    function drawAnnotations() {

        var data = google.visualization.arrayToDataTable([
          ['Task', 'Hours per Day'],
          ['Active Package',   @ViewBag.Package],
          ['SurveyPro Helping Ques.',   @ViewBag.Question],
          ['Approved Survey',   @ViewBag.ApprovedSurveyResponse],
          ['Not Approved Survey',   @ViewBag.NotApprovedSurveyResponse],
          ['SurveyPro City',   @ViewBag.City],
          ['SurveyPro State',   @ViewBag.AState],
          ['SurveyPro Ques. Category',   @ViewBag.Category],
          ['SurveyPro Attempted Review Ques.',   @ViewBag.AttemptedReviewQues],
          ['SurveyPro Attempted Survey Ques.',   @ViewBag.AttemptedSurveyQues]
        ]);

        var options = {
            title: 'SurveyPro Employee Task Chart',
            'is3D': true,
            'height': '600',
            'width': '800'
        };

        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
        chart.draw(data, options);
    }
</script>
<div id="chart_div" style="height: auto" align="center"></div>
