@model ProjSurveyPro.ProductMst
@{
    ViewBag.Title = "ProductDetails";
    Layout = "~/Views/Master/IndexLayout.cshtml";
}

<html>
<head>
    <link rel="stylesheet" href="~/Contents/appcss/theme-shop.css">
</head>
<body>
    <div role="main" class="main shop">
        <section class="page-header">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <ul class="breadcrumb">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li class="active">View Proudct</li>
                        </ul>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <h1>Product Details</h1>
                    </div>
                </div>
            </div>
        </section>
        <div class="container">
            @Html.Partial("ProductDetailsPartial",Model)
            <div class="row">
                <div style="text-align: center;">
                    <br />
                    <a class = "btn btn-primary btn-lg" href="@Url.Action("ViewProducts", "Home")">Back</a>
                </div>
            </div>
        </div>
    </div>

</body>
</html>
