    <link rel="stylesheet" href="~/Contents/assets/stylesheets/theme-custom.css">

    <!-- Head Libs -->
    <script src="~/Contents/assets/vendor/modernizr/modernizr.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script type="text/javascript" lang="javascript">
        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    $('#CompanyImage').attr('src', e.target.result);
                }
                reader.readAsDataURL(input.files[0]);
            }
        }
    </script>
</head>
<body>
    <!-- start: page -->
    <section class="body-sign">
        <div class="center-sign">
            <a href="@Url.Action("Index", "Home")" class="logo pull-left">
                <img src="~/Contents/assets/images/Logo.png"
                    height="54"
                    alt="Survey Pro"
                    class="appear-animation bounceIn appear-animation-visible"
                    data-appear-animation="bounceIn" />
            </a>

            <div class="panel panel-sign">
                <div class="panel-title-sign mt-xl text-right">
                    <h2 class="title text-uppercase text-bold m-none"><i class="fa fa-user mr-xs"></i>Sign Up</h2>
                </div>
                <div class="panel-body">
                    @using (Html.BeginForm("CompanySignUp", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        @Html.ValidationSummary(true)
                        <div class="form-group">
                            <div class="col-md-12" align="center">
                                <div class="thumb-info mb-md">
                                    <img id="CompanyImage" src="~/Contents/appimg/CompanyLogo.png" alt="Company Logo" class="img-responsive"
                                         height="220px" width="220px" text-align="center" />
                                </div>
                            </div>
                            <div class=".col-md-12" align="center">
                                <div class="fileupload fileupload-new" data-provides="fileupload">
                                    <div class="input-append">
                                        <div class="uneditable-input">
                                            @*<i class="fa fa-file fileupload-exists"></i>*@
                                            <span class="fileupload-preview"></span>
                                        </div>
                                        <span class="btn btn-default btn-file">
                                            <span class="fileupload-exists">Change</span>
                                            <span class="fileupload-new">Select Logo</span>
                                            <input type="file" name="CompanyLogo" onchange="readURL(this);" />
                                        </span>
                                    </div>
                                    @if (Session["Image"] == "1")
                                    {
                                        <h6 style="color: red;">@ViewBag.ImageType</h6>                                            
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="form-group mb-lg">
                            <label>Company Name</label>
                            @Html.TextBoxFor(model => model.CompName,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                placeholder = "Enter Company's Name"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.CompName, null, new { style = "color:red;" })
                        </div>      
                        <div class="form-group mb-lg">
                            <label>Owner Name</label>
                            @Html.TextBoxFor(model => model.OwnerName,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                placeholder = "Enter Company's Owner Name"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.OwnerName, null, new { style = "color:red;" })
                        </div>
                        <div class="form-group mb-lg">
                            <label>Email-Id</label>
                            @Html.TextBoxFor(model => model.EmailID,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                placeholder = "Enter Company's Email-Id",
                                                pattern = "[a-zA-Z0-9._%+-]+@[a-zA-Z0-9.-]+\\.[a-z]{2,4}$"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.EmailID, null, new { style = "color:red;" })
                            @if (Session["Email"] == "1")
                            {
                                <h6 style="color: red;">@ViewBag.Email</h6>
                            }
                        </div>
                        <div class="form-group mb-lg">
                            <label>Website Name</label>
                            @Html.TextBoxFor(model => model.WebsiteName,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                placeholder = "Enter Company's Website Name"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.CompName, null, new { style = "color:red;" })
                        </div>   
                        <div class="form-group mb-lg">
                            <label>Contact No</label>
                            @Html.TextBoxFor(model => model.ContactNo,
                                            new
                                            {
                                                type = "numver",
                                                @class = "form-control",
                                                placeholder = "Enter Company's Mobile No",
                                                pattern = "^[0-9]*",
                                                minlegth = 11,
                                                style="text-align:right"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.ContactNo, null, new { style = "color:red;" })
                        </div>
                        <div class="form-group mb-lg">
                            <label>Mobile No</label>
                            @Html.TextBoxFor(model => model.MobileNo,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                placeholder = "Enter another Contact No(If Any)",
                                                pattern = "^[0-9]*",
                                                minlegth = 10,
                                                style="text-align:right"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.MobileNo, null, new { style = "color:red;" })
                        </div>
                        <div class="form-group mb-lg">
                            <label>GST No</label>
                            @Html.TextBoxFor(model => model.GSTNo,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                placeholder = "Enter Company's GST No",
                                                minlegth = 10
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.MobileNo, null, new { style = "color:red;" })
                        </div>
                        <div class="form-group mb-lg">
                            <label>Company Address</label>
                            @Html.TextAreaFor(model => model.CompAddress,
                                            new
                                            {
                                                type = "text",
                                                @class = "form-control",
                                                rows = "3"
                                            }
                                        )
                            @Html.ValidationMessageFor(model => model.CompAddress, null, new { style = "color:red;" })
                        </div>
                        <div class="row">
                            <div style="text-align: center;">
                                <br />
                                @Html.TextBox("btnSubmit", "Submit", new { type = "submit", @class = "btn btn-primary btn-lg" })
                            </div>
                        </div>
                        <br />
                        <p class="text-center">Already have an account? <a href="@Url.Action("SignIn", "Home")">Sign In!</a></p>
                    }
                </div>
                <hr class="tell" />
                <div class="row">
                    <div class="header-logo">
                        <div style="text-align: center;">
                            <p>Powered By</p>
                        </div>
                        <div style="text-align: center;">
                            <img src="~/Contents/appimg/FullLogo.png"
                                width="120" height="80"
                                alt="Survey Pro" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
