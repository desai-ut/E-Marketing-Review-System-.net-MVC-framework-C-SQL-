    <script src="~/Contents/assets/vendor/jqvmap/maps/continents/jquery.vmap.south-america.js"></script>
    <script src="~/Contents/assets/vendor/select2/select2.js"></script>
    <script src="~/Contents/assets/vendor/jquery-datatables/media/js/jquery.dataTables.js"></script>
    <script src="~/Contents/assets/vendor/jquery-datatables-bs3/assets/js/datatables.js"></script>

