                                    <a>
                                        <i class="fa fa-user" aria-hidden="true"></i>
                                        <span>Employee Management</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a href="@Url.Action("Employee", "Home", new { area = "Employee" })">View Employee
                                            </a>
                                        </li>
                                    </ul>
                                </li>
