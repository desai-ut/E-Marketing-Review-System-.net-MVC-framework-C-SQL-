<body>
    <section class="body">
        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a href="@Url.Action("Index", "Home", new { area = "Employee" })" class="logo pull-left">
                    <img src="~/Contents/assets/images/Logo.png"
                        width="110" height="46"
                        alt="Survey Pro"
                        class="appear-animation bounceIn appear-animation-visible"
                        data-appear-animation="bounceIn"
                        style="animation-iteration-count: 3;"
                        data-sticky-width="115" data-sticky-height="56" data-sticky-top="33" />
                </a>
                <div class="visible-xs toggle-sidebar-left" data-toggle-class="sidebar-left-opened" data-target="html" data-fire-event="sidebar-left-opened">
                    <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                </div>
            </div>

            <!-- start: search & user box -->
            <div class="header-right">

                <span class="separator"></span>

                <ul class="notifications">
                    <li>Welcome to SurveyPro</li>
                </ul>

                <span class="separator"></span>

                <div id="userbox" class="userbox">
                    @if (Session["EmpID"] != null)
                    {
                        <a href="#" data-toggle="dropdown">
                            <figure class="profile-picture">
                                @if (Session["EmpImage"] != null)
                                {
                                    <img src="@Url.Content((string)Session["EmpImage"])" alt="Joseph Doe" class="img-circle" data-lock-picture="assets/images/%21logged-user.jpg" />
                                }
                                else
                                {
                                    <img src="~/Contents/appimg/user.png" alt="Joseph Doe" class="img-circle" data-lock-picture="assets/images/%21logged-user.jpg" />
                                }
                            </figure>
                            <div class="profile-info" data-lock-name="John Doe" data-lock-email="johndoe@okler.com">
                                @{
                                string Designation = Session["Designation"].ToString();
                                string Name = Session["Name"].ToString();
                                    <span class="name">@Name</span>
                                    <span class="role">@Designation</span>
                                }
                            </div>
                            <i class="fa custom-caret"></i>
                        </a>
                    }
                    else
                    {
                        Response.Redirect("~//Home/SignIn");
                    }
                    <div class="dropdown-menu">
                        <ul class="list-unstyled">
                            <li class="divider"></li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="@Url.Action("UpdateProfile", "Home", new { area = "Employee" })"><i class="fa fa-user"></i>My Profile</a>
                            </li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="@Url.Action("ChangePassword", "Home", new { area = "Employee" })"><i class="fa fa-lock"></i>Change Password</a>
                            </li>
                            <li>
                                @Html.ActionLink("Logout", "Index", new { area = "" })
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- end: search & user box -->
        </header>
        <!-- end: header -->
        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">

                <div class="sidebar-header">
                    <div class="sidebar-title">
                        <i class="fa fa-user" aria-hidden="true"></i>&nbsp;Employee Pannel
                    </div>
                    <div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                        <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                    </div>
                </div>

                <div class="nano">
                    <div class="nano-content">
                        <nav id="menu" class="nav-main" role="navigation">
                            <ul class="nav nav-main">
                                <li class="nav-active">
                                    <a href="@Url.Action("Index", "Home", new { area = "Employee" })">
                                        <i class="fa fa-home" aria-hidden="true"></i>
                                        <span>Dashboard</span>
                                    </a>
                                </li>
                                <li class="nav-parent">
                                    <a>
                                        <i class="fa fa-file text" aria-hidden="true"></i>
                                        <span>Package Management</span>
                                    </a>
