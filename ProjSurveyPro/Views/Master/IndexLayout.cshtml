                    <div class="row">
                        <div class="col-md-1">
                            <a href="index.html" class="logo">
                                <img alt="Porto Website Template" class="img-responsive" src="/Contents/appimg/Logo.png">
                            </a>
                        </div>
                        <div class="col-md-7">
                            <p>© Copyright 2017. All Rights Reserved.</p>
                        </div>
                        <div class="col-md-4">
                            <nav id="sub-menu">
                                <ul>
                                    <li>@Html.ActionLink("FAQ's", "FAQ")</li>
                                    <li>@Html.ActionLink("Reach Us", "ReachUs")</li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
