                                                <li class="">
                                                    @Html.ActionLink("About System", "AboutSystem")
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Meet Us", "MeetUs")
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Reach Us", "ReachUs")
                                                </li>
                                                <li class="dropdown">
                                                    <a href="#" class="dropdown-toggle">Sign Up</a>
                                                    <ul class="dropdown-menu">
                                                        <li>@Html.ActionLink("User Sign Up", "UserSignUp")</li>
                                                        <li>@Html.ActionLink("Company Sign Up", "CompanySignUp")</li>
                                                    </ul>
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Sign In", "SignIn")
                                                </li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <div>
            @RenderBody();
        </div>

        <footer id="footer">
            <div class="container">
                <div class="row">
                    <div class="footer-ribbon">
                        <span>Get in Touch</span>
                    </div>
                    <div class="col-md-3"></div>
                    <div class="col-md-3"></div>
                    <div class="col-md-4">
                        <div class="contact-details">
                            <h4>Contact Us</h4>
                            <ul class="contact">
                                <li>
                                    <p><i class="fa fa-map-marker"></i><strong>Address:</strong> 9,Shraddha Soc, Vesu, Surat, India</p>
                                </li>
                                <li>
                                    <p><i class="fa fa-phone"></i><strong>Phone:</strong> (+91) 7575040697</p>
                                </li>
                                <li>
                                    <p><i class="fa fa-envelope"></i><strong>Email:</strong> <a href="mailto:surveypro441@gmail.com">surveypro441@gmail.com</a></p>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <h4>Follow Us</h4>
                        <ul class="social-icons">
                            <li class="social-icons-facebook"><a href="@Url.Action("http://www.facebook.com/")" target="_blank" title="Facebook"><i class="fa fa-facebook"></i></a></li>
