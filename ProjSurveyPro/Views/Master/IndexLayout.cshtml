                                                    @Html.ActionLink("User", "Index", "Home", new { area = "User" }, null)
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Client", "Index", "Home", new { area = "Client" }, null)
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Employee", "Index", "Home", new { area = "Employee" }, null)
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Admin", "Index", "Home", new { area = "Admin" }, null)
                                                </li>*@
                                                <li class="">
                                                    @Html.ActionLink("Home", "Index")
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Our Service", "Service")
                                                </li>
                                                <li class="">
                                                    @Html.ActionLink("Packages", "ViewPackages")
                                                </li>
                                                <li class="">
